		xmlns:forms="sap.ui.layout.form"
		xmlns:core="sap.ui.core"
		xmlns="sap.m">
	<l:VerticalLayout class="sapUiContentPadding">
		<ObjectHeader
			title="{i18n>bilgiUni}"
			icon="images/uniLogos/bilgiUni.png">
			<attributes>
				<ObjectAttribute text="2015 - 2017" />
				<ObjectAttribute text="Master of Business Administration (MBA)" />
			</attributes>
		</ObjectHeader>
		<ObjectHeader
			title="{i18n>sofiaUni}"
			number=""
			icon="images/uniLogos/sofiaUni.png">
			<attributes>
				<ObjectAttribute text="2007 - 2012" />
				<ObjectAttribute text="Computer Engineering" />
			</attributes>
		</ObjectHeader>
	</l:VerticalLayout>
</mvc:View>
