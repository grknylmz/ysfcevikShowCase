{
	"_version":"1.12.0",
	 "sap.app":{
	 		"id":"sapui5.ycevik",
	 		"type":"application",
	 		"i18n":"i18n/i18n.properties",
	 		"applicationVersion":{
	 			"version":"1.0.0"
	 			
	 		},
	 		"title":"{{appTitle}}",
	 		"description":"{{appDescription}}",
	 		"sourceTemplate":{
	 			"id":"ui5template.basicSAPUI5ApplicationProject",
	 			"version":"1.40.12"
	 			
	 		}
	 	
	 },
 	"sap.ui":{
 		"technology":"UI5",
 		"icons":{
 			"icon":"",
 			"favIcon":"",
 			"phone":"",
 			"phone@2":"",
 			"tablet":"",
 			"tablet@2":""
 			
 		},
 		"deviceTypes":{
 			"desktop":true,
 			"tablet":true,
 			"phone":true
 		}
 	},
 	"sap.ui5":{
 		"flexEnabled":false,
 		"rootView":{
 			"viewName":"sapui5.ycevik.view.App",
 			"type":"XML",
 			"async":true,
 			"id":"App"
 		},
 		"dependencies":{
 			"minUI5Version":"1.60.1",
 			"libs":{
 				"sap.ui.layout":{},
 				"sap.ui.core":{},
 				"sap.m":{}
 			}
 		},
 		"contentDensities":{
 			"compact":true,
 			"cozy":true
 		},
 		"models":{
 			"i18n":{
 				"type":"sap.ui.model.resource.ResourceModel",
 				"settings":{
 					"bundleName":"sapui5.ycevik.i18n.i18n"
 				}
 			},
		"products": {
			"type": "sap.ui.model.json.JSONModel",
			"uri": "model/products.json"
	  }
 		},
 		"resources":{
 			"css":[{
 				"uri":"css/style.css"
 			}]
 		},
 		"routing":{
 			"config":{
 				"routerClass":"sap.m.routing.Router",
 				"viewType":"XML",
 				"async":true,
 				"viewPath":"sapui5.ycevik.view",
 				"controlAggregation":"pages",
 				"controlId":"app",
 				"clearControlAggregation":false
 			},
 			"routes":[{
 				"name":"Tiles",
 				"pattern":"",
 				"target":["Tiles"]
 			},
 			{
 				"name":"Personal",
 				"pattern":"Personal",
 				"target":["Personal"]
 			},
 			{
 				"name":"Worklist",
 				"pattern":"Worklist",
 				"target":["Worklist"]
 			},
 			{
 				"name":"Object",
 				"pattern":"Object/{objectId}",
 				"target":["Object"]
 			}
 			],
 			"targets":{
 				"TargetApp":{
 					"viewType":"XML",
 					"transition":"slide",
 					"clearControlAggregation":false,
 					"viewId":"App",
 					"viewName":"App"
 				},
 				"Tiles":{
 					"viewType":"XML",
 					"viewName":"Tiles"
 				},
 				"Personal":{
 					"viewType":"XML",
 					"viewName":"personal.Personal"
 				},
 				"Worklist":{
 					"viewType":"XML",
 					"viewName":"worklist.Worklist"
 				},
 				"Object":{
 					"viewType":"XML",
 					"viewName":"worklist.Objectt"
 				}
 			}
 		}
 	}
}